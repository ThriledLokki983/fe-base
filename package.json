{
  "name": "fe-base",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "A base template for React applications using Vite and TypeScript + SWC. ",
  "author": "Gideon Nimoh",
  "scripts": {
    "start": "vite --port=${VITE_PORT:-3000} --host",
    "start:dev": "vite --mode development --port=${VITE_PORT:-3000} --host",
    "start:staging": "vite --mode staging --port=${VITE_PORT:-3001} --host",
    "start:prod": "vite --mode production --port=${VITE_PORT:-80} --host",
    "watch": "tsc -b --watch",
    "test": "vite test",
    "test:unit": "vitest --run",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  },
  "main": "eslint.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThriledLokki983/fe-base.git"
  },
  "keywords": [],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ThriledLokki983/fe-base/issues"
  },
  "homepage": "https://github.com/ThriledLokki983/fe-base#readme"
}
